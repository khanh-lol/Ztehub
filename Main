local q=game.Players.LocalPlayer


local function savePosition()
local r=q.Character
if r then
local s=r:FindFirstChild"HumanoidRootPart"
if s then
p=s.Position
end
end
end


local function resetAndTeleportBack()
local r=q.Character
if r then
local s=r:FindFirstChildOfClass"Humanoid"
if s then
s.Health=0
q.CharacterAdded:Wait()
wait(0.1)

r=q.Character
local t=r:FindFirstChild"HumanoidRootPart"
if t and p then
t.CFrame=CFrame.new(p)
end
end
end
end


